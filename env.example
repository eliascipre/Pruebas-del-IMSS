# Variables de entorno para la Suite IMSS
# Copia este archivo como .env y configura tus valores

# Configuración de MedGemma
MEDGEMMA_ENDPOINT=https://your-medgemma-endpoint.com
LM_STUDIO_URL=http://lm-studio:1234

# Configuración de Gemini
GEMINI_API_KEY=your-gemini-api-key-here
GCP_MEDGEMMA_ENDPOINT=https://your-gcp-medgemma-endpoint.com
GCP_MEDGEMMA_SERVICE_ACCOUNT_KEY={"type": "service_account", "project_id": "your-project"}

# Configuración de la base de datos
REDIS_URL=redis://redis:6379

# Configuración de desarrollo
NODE_ENV=development
FLASK_ENV=development

# Configuración de logging
LOG_LEVEL=INFO

# Configuración de seguridad
SECRET_KEY=your-secret-key-here
JWT_SECRET=your-jwt-secret-here

# Configuración de CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:5001,http://localhost:5002,http://localhost:5003,http://localhost:5004
